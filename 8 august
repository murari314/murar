public class TimeTable {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		for (int x = 1; x <= 9; x++) {
	         System.out.print(x +":");
	         for (int y = 1; y <= 9; y++) {
	            System.out.print(x*y +" ");
	         }
	         System.out.println();
	      }
    }
            
    

	}



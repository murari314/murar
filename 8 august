public class oddNumber {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
int rem=0;
int odd=0;
for(odd=0;odd<=100;odd++){
	rem=odd%2;
	if(rem!=0){
		System.out.println(odd);
	}
	
}
	}

}

